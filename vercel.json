{
    "version": 2,
    "builds": [
        {
            "src": "server/api/index.ts",
            "use": "@vercel/node"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/public/index.html"
      }
    ]
  }
  